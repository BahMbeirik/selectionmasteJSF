	<navigation-rule>
	    <from-view-id>candidat/success.xhtml</from-view-id>
	    <navigation-case>
	        <from-outcome>candidat/success</from-outcome>
	        <to-view-id>candidat/success.xhtml</to-view-id>
	    </navigation-case>
